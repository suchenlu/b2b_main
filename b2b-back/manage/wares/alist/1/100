	<script src="../../../../static/js/jquery.js"></script>
	<script src="../../../../static/js/bootstrap.min.js"></script>
	<script>
	$('.operate1 a').click(function(){
		$('.modal-title').html('');
       	$('.modal-body').html('');
		var text=$(this).text();
		var title="";
		var html="";
		if(text=="OP币设置"){
			title=text;
			html="<form action='' method='post'>"
	        +"<input type='radio' name='commission' value='0' checked /> 自动结算 "
	        +"<input type='radio' name='commission' value='1'/> 手动结算"
	        +"<button class='btn btn-sm block' type='submit'>保存设置</button></form>"
	       
		}else if(text=="现金立减"){
			title=text+"设置";
			html="<form action='' method='post'>"
        +"<input type='text'name='cash_reward' placeholder='当前值 : 0.00' value=''>RMB"
        +"<button class='btn btn-sm block' type='submit'>保存设置</button></form>"
		}else if(text=="结算方法"){
			title=text+"设置";
			html="<form action='' method='post'>"
        +"<input type='radio' name='commission' value='0' checked/> 自动结算"
        +"<input type='radio' name='commission' value='1'/> 手动结算"
        +"<button class='btn btn-sm block' type='submit'>保存设置</button></form>"
		}else{
			return false
		}
	   $('.modal-title').append(title);
       $('.modal-body').append(html);
       $('#myModal').animate({'opacity':1,'top':5+'%'}).show()
	     $('.close,.cancel').click(function(){
	        $('#myModal').animate({'top':-30+'%','opacity':0}).slideUp();
	     })
	})
	</script>
	<script>
	    $(function(){   
	        var tab_width=$(".table").width();
	        $(".nav").width(tab_width + 20);
	        $(".nav-tabs").width(tab_width);
	    })
	</script>
