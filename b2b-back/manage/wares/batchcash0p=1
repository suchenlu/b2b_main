<script src="../../static/js/bootstrap.min.js"></script>

    <script src="../../static/js/jquery.min.js" type="text/javascript"></script>
    <script src="../../static/js/jquery-ui-1.92.js" type="text/javascript"></script>
    <script src="../../static/js/jquery-ui-timepicker-addon.js" type="text/javascript"></script>

    <!--中文-->
    <script src="../../static/js/jquery.ui.datepicker-zh-CN.js" type="text/javascript" ></script>
    <script src="../../static/js/jquery-ui-timepicker-zh-CN.js" type="text/javascript"></script>
    <script>
        $(document).ready(function(){
            var $div = $('#viewmess');
            var $tr = $('<tr></tr>');
            var $tbody = $('<tbody></tbody');
            $div.hide();
            $("select[name='commission_id']").change(function() {
                    if ($tbody.html() != '') {
                        $('#viewmess tbody  tr').remove();
                    }
                    $.get('../../manage/getcommistion/' + $(this).val(), function (data, s) {

                        $tr.html('');

                        for (var i = 0; i < data.data.length; i++) {
                            var $td = $('<td></td>');
                            $td.text(data.data[i]);
                            $tr.append($td);

                        }
                        $tr.appendTo($tbody.html(''));
                        $tbody.appendTo($('#viewmess > table'));
                        $div.show();
                    }, 'json')
                })

                $("select[name='commission_id']").blur(function () {
                    $div.css('display', 'none');
                })
            $("#cron_date").datetimepicker({
                        timeFormat: "HH:mm:ss",
                        dateFormat: "yy-mm-dd"
            });
            $(function () {
                $('#setop').click(function(){
                var $op = $("input[name='setOP']").val();
                var $cash = $("input[name='setCash']").val();
                if (parseFloat($op) < 0 || parseFloat($cash) < 0){
                    alert('op 币 和现金立返必须大于 0！');
                    return false;
                }
                });
            })



        });
    </script>


  </body>
</html>
