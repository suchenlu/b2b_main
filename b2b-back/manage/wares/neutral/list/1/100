    <script type="text/javascript">
      $(document).ready(function(){
          var img_pop = $( ".img_pop" );
          $( ".s_picture" ).click( function(){
              var src = $(this).find("img").attr( "src" );
              img_pop.find("img").attr( "src", src );
              img_pop.fadeIn( 200 );
          } );
          $(".img_pop").click(function(){
              $('.img_pop').fadeOut();
          });
      })
  </script>
