    <script>
        $(function () {
            $("#submit").click(function () {

                var id = document.getElementById("id").value;
                var peration = document.getElementById("peration").value;
                var part_name = document.getElementById("part_name").value;
                var start = document.getElementById("jy").value;
                var end = document.getElementById("jj").value;
                if (id == "" && start == "" && end == "" && peration == "" && part_name == "") {
                    alert("请至少输入一个查询条件");
                    return false;
                }
                else {
                    if (!istel(id) && id != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(peration) && peration != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(part_name) && part_name != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    return true;

                }

                function toDate(str) {
                    var date = str.split("-");
                    return new Date(date[0], date[1], date[2]);
                }

                var start = toDate($("#jy").val());
                var end = toDate($("#jj").val());
                if (start > end) {
                    alert("起始日期必须小于结束日期");
                    return false;
                }
            })
            $("#part_name").attr("placeholder", "请输入正确分销商名称");
            $("#id").attr("placeholder", "请输入正确订单号");
            function istel(tel) {
                var num1 = /[A-Za-z].*[0-9]|[0-9].*[A-Za-z]/;
                return (num1.test(tel));
            }
            $("#peration").attr("placeholder", "请输入正确操作帐户")
            function isdata(text) {
                var reg = /^[\u4e00-\u9fa5a-zA-Z0-9]+$/;
                return (reg.test(text));
            }
        })

    </script>
