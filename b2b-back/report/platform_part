    <script>
        $(function () {
            $("#submit").click(function () {

                var org_name = document.getElementById("org_name").value;
                var site = document.getElementById("site").value;
                var partner = document.getElementById("partner").value;
                if (org_name == "" && partner == "" && site == "") {
                    alert("请至少输入一个查询条件");
                    return false;
                }
                else {
                    if (!istext(partner) && partner != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!istext(org_name) && org_name != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(site) && site != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    return true;

                }
            })
            $("#org_name").attr("placeholder", "请输入正确组织名称");
            $("#partner").attr("placeholder", "请输入正确所属商家");
            $("#site").attr("placeholder", "请输入正确站点名称");
            function istext(text) {
                var reg = /^[\u4e00-\u9fa5a-zA-Z0-9]+$/;
                return (reg.test(text));
            }
            function isdata(text) {
                var reg = /[^\x00-\x80]/;
                return (reg.test(text));
            }

        })

    </script>

