    <script>
        $(function () {
            $("#submit").click(function () {

                var id = document.getElementById("id").value;
                var partner_name = document.getElementById("partner_name").value;
                var partner_id = document.getElementById("partner_id").value;
                var part_name = document.getElementById("part_name").value;
                var part_id = document.getElementById("part_id").value;
                var site = document.getElementById("site").value;
                var start = document.getElementById("jy").value;
                var end = document.getElementById("jj").value;
                var first = document.getElementById("jy1").value;
                var end1 = document.getElementById("jj1").value;
                var second = document.getElementById("jy2").value;
                var end2 = document.getElementById("jj2").value;
                var date_depart = document.getElementById("jy3").value;
                var end3 = document.getElementById("jj3").value;
                var date_return = document.getElementById("jy4").value;
                var end4 = document.getElementById("jj4").value;
                var payment = document.getElementById("jy5").value;
                var end5 = document.getElementById("jj5").value;
                var pay_id = document.getElementById("pay_id").value;
                var pay_type = document.getElementById("pay_type").value;
                var pay_channel = document.getElementById("pay_channel").value;

                if (id == "" && site == "" && start == "" && end == "" && partner_name == "" && partner_id == "" && part_name == "" && part_id == "" && first == '' && second == '' && end1 == '' && end2 == '' && end3 == '' && end4 == '' && end5 == '' && payment == '' && date_depart == '' && date_return == '' && pay_id == '' && pay_channel == '' && pay_type == '') {
                    alert("请至少输入一个查询条件");
                    return false;
                }
                else {
                    if (!istext(site) && site != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }

                    if (!istel(id) && id != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }

                    if (!isdata(partner_name) && partner_name != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(partner_id) && partner_id != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(part_name) && part_name != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(pay_channel) && pay_channel != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(pay_id) && pay_id != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(pay_type) && pay_type != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    if (!isdata(part_id) && part_id != "") {
                        alert("没有匹配的查询结果");
                        return false;
                    }
                    return true;
                }
                function toDate(str) {
                    var date = str.split("-");
                    return new Date(date[0], date[1], date[2]);
                }

                var start = toDate($("#jy").val());
                var end = toDate($("#jj").val());
                if (start > end) {
                    alert("起始日期必须小于结束日期");
                    return false;

                }
                var first = toDate($("#jy1").val());
                var end1 = toDate($("#jj1").val());
                if (first > end1) {
                    alert("起始日期必须小于结束日期");
                    return false;

                }
                var second = toDate($("#jy2").val());
                var end2 = toDate($("#jj2").val());
                if (second > end2) {
                    alert("起始日期必须小于结束日期");
                    return false;

                }
                var date_depart = toDate($("#jy3").val());
                var end3 = toDate($("#jj3").val());
                if (date_depart > end3) {
                    alert("起始日期必须小于结束日期");
                    return false;

                }
                var date_return = toDate($("#jy4").val());
                var end4 = toDate($("#jj4").val());
                if (date_return > end4) {
                    alert("起始日期必须小于结束日期");
                    return false;

                }
                var payment = toDate($("#jy5").val());
                var end5 = toDate($("#jj5").val());
                if (payment > end5) {
                    alert("起始日期必须小于结束日期");
                    return false;

                }

            })
            $("#site").attr("placeholder", "请输入正确站点名称");
            $("#id").attr("placeholder", "请输入正确订单号");
            $("#partner_name").attr("placeholder", "请输入正确供应商名称");
            $("#partner_id").attr("placeholder", "请输入正确供应商ID");
            $("#part_name").attr("placeholder", "请输入正确分销商名称");
            $("#part_id").attr("placeholder", "请输入正确分销商ID");
            $("#pay_id").attr("placeholder", "请输入正确支付流水号");
            $("#pay_channel").attr("placeholder", "请输入正确支付渠道");
            $("#pay_type").attr("placeholder", "请输入正确支付类型");
            function istext(text) {
                var reg = /[^\x00-\x80]/;
                return (reg.test(text));
            }

            function istel(tel) {
                var num1 = /[A-Za-z].*[0-9]|[0-9].*[A-Za-z]/;
                return (num1.test(tel));
            }

            function isdata(text) {
                var reg = /^[\u4e00-\u9fa5a-zA-Z0-9]+$/;
                return (reg.test(text));
            }
        })
    </script>
